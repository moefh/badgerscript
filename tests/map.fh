
function print_map(map)
{
  var map_len = len(map);
  printf("{\n");
  var k = next_key(map, null);
  while (k != null) {
    printf("  %s : %s,\n", k, map[k]);
    k = next_key(map, k);
  }
  printf("}\n");
}

