
function print_array(arr)
{
  var arr_len = len(arr);
  printf("array has %d elements:\n", arr_len);
  var i = 0;
  while (i < arr_len) {
    printf("  [%d] = %s\n", i, arr[i]);
    i = i + 1;
  }
}

function main(args)
{
   var x = [1,2,3];

   printf("BEFORE: ");
   print_array(x);

   x[0] = "hello, world!";

   printf("AFTER: ");
   print_array(x);
}

