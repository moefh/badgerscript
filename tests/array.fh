
function print_array(arr)
{
  printf("array has %d elements:\n", len(arr));
  var i = 0;
  while (i < len(arr)) {
    printf("  [%d] = %s\n", i, arr[i]);
    i = i + 1;
  }
}

function main(args)
{
   printf("BEFORE: ");
   print_array(args);

   args[0] = "hello, world!";

   printf("AFTER: ");
   print_array(args);
}

