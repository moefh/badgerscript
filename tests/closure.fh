
function make_counter(c)
{
  return [
    function() { return c; },
    function(n) { c = c + n; }
  ];
}

function main()
{
  var c1 = make_counter(0);
  var c2 = make_counter(100);

  c1[0](1);
  printf("c1=%d\n", c1[1]());

  c2[0](20);
  printf("c2=%d\n", c2[1]());

  printf("c1=%d\n", c1[1]());
}
