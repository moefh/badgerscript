
function print() {}

function f(x, y) {
  # expressions
  var z = x + y;
  y = 2 * x + 0.5;

  # function call
  x = print(x, 5) + print(print(1,2), 0);

  # if
  if (x == 42)
    print("jackpot\n");
  else
    print("no luck\n");
  print("hello, world!\n");

  # block
  {
    x = 1;
    f(0,1);
  }

  # loop
  var i = 0;
  while (1) {
    if (i == 5)
      continue;
    if (i > 10)
      break;
    print(i);
    i = i + 1;
  }

  return x + 1;
}

function main(args) {
  var x = 1;
  print("hello, world", f(x, 0) + 1.5, "\n");
  return 0;
}
