
function f(x, y) {
  x = 2 * x + 0.5;
  print("hello, world!\n");
  return f(x,1) + 2 * y;
  {
    x = 1;
  }
  if (x >= 42)
    print("jackpot\n");
  else
    print("no luck\n");
  x = 0;
  while (true) {
    if (x == 5)
      continue;
    if (x > 10)
      break;
    print(x);
    x = x + 1;
  }
}

function main(args) {
  var x = 1;
