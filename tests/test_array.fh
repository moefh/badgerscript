
function print_array(arr)
{
  var arr_len = len(arr);
  printf("array has %d elements:\n", arr_len);
  var i = 0;
  while (i < arr_len) {
    printf("  [%d] = %s\n", i, arr[i]);
    i = i + 1;
  }
}

